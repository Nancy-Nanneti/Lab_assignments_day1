package labassignment_day1;
public class fibonacci {
	static int fib(int n) {
		if(n<=1)
			return 1;
		return fib(n-1)+fib(n-2);
	}
  public static void main(String args[]) {
	  int sum=0;
	  for(int i=0;i<20;i++) {
		  System.out.print(fib(i)+" ");
		  sum+=fib(i);
	  }
	  System.out.print("\n");
	  System.out.print("The average is "+(float)sum/20);
  }
}
