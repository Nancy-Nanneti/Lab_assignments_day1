package labassignment_day1;
public class timeTable{
	public static void main(String[] args) {
		System.out.println("*| 1 2 3 4 5 6 7 8 9 ");
		System.out.println("---------------------");
		for(int i=1;i<=9;i++) {
			System.out.println();
			for(int j=1;j<=9;j++) {
				//System.out.println();
				System.out.print(" "+i*j);
				//System.out.println();
			}
		}
		
  }
}
